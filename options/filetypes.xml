      <mapping pattern=".gitkeep" type="PLAIN_TEXT" />
      <mapping pattern=".Xdefaults" type="PLAIN_TEXT" />
      <mapping ext="conf" type="PLAIN_TEXT" />
      <mapping pattern="pydecruft" type="Shell Script" />
      <mapping pattern=".zshrc" type="Shell Script" />
      <mapping pattern=".bashrc" type="Shell Script" />
    </extensionMap>
  </component>
</application>